	}
}

function fillGridTypeInput() {
	$('#gridTypeInput option.custom').remove();
	if(currentDeviceType == 0 && currentPhase == 3) {
		$('#gridTypeInput').append('<option class="custom" value="0">0: 230V/230V</option>');
		$('#gridTypeInput').append('<option class="custom" value="1">1: 120V/208V</option>');
		$('#gridTypeInput').append('<option class="custom" value="2">2: 240V/415V</option>');
		$('#gridTypeInput').append('<option class="custom" value="3">3: 230V/400V</option>');
		$('#gridTypeInput').append('<option class="custom" value="4">4: Rsvd</option>');
		$('#gridTypeInput').append('<option class="custom" value="5">5: 220V/380V</option>');
	} else {
		$('#gridTypeInput').append('<option class="custom" value="0">0: 240V/120V</option>');
		$('#gridTypeInput').append('<option class="custom" value="1">1: 208V/120V</option>');
		$('#gridTypeInput').append('<option class="custom" value="2">2: 240V</option>');
		$('#gridTypeInput').append('<option class="custom" value="3">3: 230V</option>');
		$('#gridTypeInput').append('<option class="custom" value="4">4: 200V/100V</option>');
		if((currentDeviceType == 6 && currentSlaveVersion >= 0x1F && currentFwVersion >= 0x1F) || isMega24K()) {
			$('#gridTypeInput').append('<option class="custom" value="5">5: 220V/127V</option>');
		}
	}
}

