		if(currentPhase == 3 && currentDeviceType == 0) {			//Trip 12K...
			$('#setMasterOrSlaveInput').append('<option class="custom" value="3">' + slaveHybrid1TextTrip12K + '</option>');
			$('#setMasterOrSlaveInput').append('<option class="custom" value="2">' + slaveHybrid2Text + '</option>');
		} else if((currentDeviceType == 8 && currentDtc != 1) || currentDeviceType == 10) {
			$('#setMasterOrSlaveInput').append('<option class="custom" value="1">' + slaveHybrid1Text + '</option>');
			$('#setMasterOrSlaveInput').append('<option class="custom" value="2">' + slaveHybrid2Text + '</option>');
			$('#setMasterOrSlaveInput').append('<option class="custom" value="3">' + slaveHybrid3Text + '</option>');
		} else{
			$('#setMasterOrSlaveInput').append('<option class="custom" value="1">' + slaveHybrid1Text + '</option>');
			$('#setMasterOrSlaveInput').append('<option class="custom" value="2">' + slaveHybrid2Text + '</option>');
			$('#setMasterOrSlaveInput').append('<option class="custom" value="3">' + slaveHybrid3Text + '</option>');
			$('#setMasterOrSlaveInput').append('<option class="custom" value="4">' + slaveHybrid4Text + '</option>');
		}
	}
}

function checkFuncNPEHolder() {
	if(isAdmin || isMaintain || isEG4Admin || isDevAccount) {
		$('.funcNPEHolder').show();
	} else if((currentDeviceType == 8 || currentDeviceType == 10) && !isViewer) {
		$('.funcNPEHolder').show();
	} else {
		$('.funcNPEHolder').hide();
	}
}