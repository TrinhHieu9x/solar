	}  else if(isMega24K()) {
		$('#meterBrandModelInput').append('<option class="custom" value="0">' + meterBrand0 + '</option>');
		$('#meterBrandModelInput').append('<option class="custom" value="0">' + meterBrand1_2 + '</option>');
	} else {
