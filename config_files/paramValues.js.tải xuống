function fillEpsVoltageSetInput() {
	if(isSnaSeries()) {			//Off-Grid
		$('#epsVoltageSetInput option.custom').remove();
		if(currentDtc == 1) {				//Split
			$('#epsVoltageSetInput').append('<option class="custom" value="127">127/220 three phase</option>');
			$('#epsVoltageSetInput').append('<option class="custom" value="200">100/200 split phase</option>');
			$('#epsVoltageSetInput').append('<option class="custom" value="208">120/208</option>');
			$('#epsVoltageSetInput').append('<option class="custom" value="220">110/220</option>');
			$('#epsVoltageSetInput').append('<option class="custom" value="230">115/230</option>');
			$('#epsVoltageSetInput').append('<option class="custom" value="240">120/240</option>');
		} else {
			$('#epsVoltageSetInput').append('<option class="custom" value="120">120</option>');
			$('#epsVoltageSetInput').append('<option class="custom" value="200">200</option>');
			$('#epsVoltageSetInput').append('<option class="custom" value="208">208</option>');
			$('#epsVoltageSetInput').append('<option class="custom" value="220">220</option>');
			$('#epsVoltageSetInput').append('<option class="custom" value="230">230</option>');
			$('#epsVoltageSetInput').append('<option class="custom" value="240">240</option>');
		}
	}
}

function fillCtSampleRatioInput() {
	$('#ctSampleRatioInput option.custom').remove();
	$('#ctSampleRatioInput').append('<option class="custom" value="0">1/1000</option>');
	$('#ctSampleRatioInput').append('<option class="custom" value="1">1/3000</option>');
	$('#ctSampleRatioInput').append('<option class="custom" value="2">1/2000</option>');
	if(currentDeviceType == 6 && (currentStandard == 'FAAB' || currentStandard == 'fAAB') && currentSlaveVersion >= 0x1F && currentFwVersion >= 0x1F || currentDeviceType == 8 && (currentStandard == 'HAAA' || currentStandard == 'hAAA') && currentSlaveVersion >= 0x08 && currentFwVersion >= 0x07) {
		$('#ctSampleRatioInput').append('<option class="custom" value="3">1/4000</option>');
		$('#ctSampleRatioInput').append('<option class="custom" value="4">1/6000</option>');
	}
}

function fillMeterBrandSetInput() {
	$('#meterBrandModelInput option.custom').remove();
	
	if(currentDeviceType == 6) {
		$('#meterBrandModelInput').append('<option class="custom" value="0">' + meterBrand0 + '</option>');
		$('#meterBrandModelInput').append('<option class="custom" value="1">' + meterBrand1_2 + '</option>');
		$('#meterBrandModelInput').append('<option class="custom" value="2">' + meterBrand2 + '</option>');
		$('#meterBrandModelInput').append('<option class="custom" value="3">' + meterBrand3 + '</option>');
	} else if(currentDeviceType == 10) {
		$('#meterBrandModelInput').append('<option class="custom" value="0">' + meterBrand0 + '</option>');
		$('#meterBrandModelInput').append('<option class="custom" value="1">' + meterBrand1_1 + '</option>');
