	var currentUsVersion = -1;

	var firstLoad = false
	
		firstLoad = true

		currentModuleInverterSn = '3233630034';
		currentDatalogSn = 'BA32007412';
		currentProtocolVersion = '5';
		currentPhase = 1;
		currentDeviceType = 3
		currentSubDeviceType = -1;
		currentPowerRating = 4;
		currentDtc = 0;
		currentVoltClass = 0;
		currentMachineType = 0;
		currentMeterType = 0;
		currentParallelEnabled = false;
		currentMasterOrSlaveNotOffGrid = 0;
		currentComposedPhaseNotOffGrid = 1;
		currentStandard = 'cbaa';
		currentSlaveVersion = 25;
		currentFwVersion = 22;
		currentHardwareVersion = 10;
		currentBatteryType = 'LITHIUM';
		currentWithbatteryData = false;
		currentBindTigoInfo = false;
		currentBindPhnixInfo = false;
		currentBindChargePoint = false;
		currentAllowExport2Grid = true;
		currentSetG98Already = false;
