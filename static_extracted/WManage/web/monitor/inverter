	$(document).ready(function () {
		if (typeof (initPageWhenReady) == "function") {
			initPageWhenReady();
		}

		
			if(firstLoad) {
				refreshUserVisitRecordAndNotice('3233630034', 122555);
			}
		

		$('#selectPlantFirstCheckbox').change(function () {
			if ($('#selectPlantFirstCheckbox').is(':checked')) {
				$('#plantSearchHolder').fadeIn(refreshPlantComboAndChart);
			} else {
				$('#plantSearchHolder').fadeOut(refreshInverterComboAndChart);
			}
		});

		function refreshPlantComboAndChart() {
			firstPlantSearchSuccess = true;
			firstInverterSearchSuccess = true;
			$('#plantSearchInput').combogrid('setValue', '');
			$('#plantSearchInput').combogrid('grid').datagrid('load');
		}

		var firstPlantSearchSuccess = true;
		var plantSearchTextBeforeLoad = '';
		$('#plantSearchInput').combogrid({
			panelWidth: 530,
			url: plantListUrl,
			idField: 'id',
			textField: 'name',
			mode: 'remote',
			delay: 500,
			fitColumns: true,
			pagination: true,
			pageSize: 20,
			onBeforeLoad: function (param) {
				plantSearchTextBeforeLoad = $('#plantSearchInput').combogrid('getText');
				param.searchText = plantSearchTextBeforeLoad;
				param.targetPlantId = userVisitRecordPlantId;
				param.sort = 'createDate';
				param.order = 'desc';
			},
			onSelect: function (index, row) {
				selectPlantId = row.id;
				selectPlantName = row.name;

				if (typeof (onPlantChanged) == "function") {
					onPlantChanged(selectPlantId, selectPlantName);
				}

				if (firstPlantSearchSuccess) {
					firstPlantSearchSuccess = false;
				} else {
					refreshInverterComboAndChart();
				}
			},
			onLoadSuccess: function (data) {
				$('#plantSearchInput').combogrid('setText', plantSearchTextBeforeLoad)
				if (firstPlantSearchSuccess) {
					if (data.total > 0) {
						var plantSearchGrid = $('#plantSearchInput').combogrid('grid');
						plantSearchGrid.datagrid('selectRecord', data.rows[0].id);
					} else {
						firstPlantSearchSuccess = false;
					}

					initInverterSearchInput();
				}
			},
			columns: [[
				{
					field: 'name',
					title: 'Station name',
					formatter: plantSearchNameFormatter,
					width: 80
				}
			]]
		});

		function refreshInverterComboAndChart() {
			firstInverterSearchSuccess = true;
			$('#inverterSearchInput').combogrid('setValue', '');
			$('#inverterSearchInput').combogrid('grid').datagrid('load');
		}

		var firstInverterSearchSuccess = true;

		function initInverterSearchInput() {
			$('#inverterSearchInput').combogrid({
				panelWidth: 530,
				url: baseUrl + '/web/config/inverter/list',
				idField: 'serialNum',
				textField: 'serialNum',
				mode: 'remote',
				delay: 500,
				fitColumns: true,
				pagination: true,
				pageSize: 20,
				onBeforeLoad: function (param) {
					if ($('#selectPlantFirstCheckbox').is(':checked')) {
						var plantSearchGridSelected = $('#plantSearchInput').combogrid('grid').datagrid('getSelected');
						if (!plantSearchGridSelected) {
							return false;
						}
						param.plantId = plantSearchGridSelected.id;
					}
					param.searchText = $('#inverterSearchInput').combogrid('getValue');
					param.targetSerialNum = userVisitRecordSerialNum;
				},
				onSelect: function (index, row) {
					currentModuleInverterSn = row.serialNum;
					currentDatalogSn = row.datalogSn;
					currentProtocolVersion = row.protocolVersion;
					currentPhase = row.phase;
					currentDeviceType = row.deviceType;
					currentSubDeviceType = row.subDeviceType
					currentPowerRating = row.powerRating;
					currentDtc = row.dtc;
					currentVoltClass = row.voltClass;
					currentMachineType = row.machineType;
					currentMeterType = row.meterType;
					currentParallelEnabled = row.parallelEnabled;
					currentMasterOrSlaveNotOffGrid = (currentParallelEnabled ? row.masterOrSlave : -1);
					currentComposedPhaseNotOffGrid = (currentParallelEnabled ? row.composedPhase : -1);
					currentStandard = row.standard;
					currentSlaveVersion = row.slaveVersion;
					currentFwVersion = row.fwVersion;
					currentHardwareVersion = row.hardwareVersion
					currentBatteryType = row.batteryType;
					currentWithbatteryData = row.withbatteryData;
					currentBindTigoInfo = row.bindTigoInfo
					currentBindPhnixInfo = row.bindPhnixInfo
					currentBindChargePoint = row.bindChargePoint
					currentAllowExport2Grid = row.allowExport2Grid
					currentSetG98Already = row.setG98Already
					currentOdm = row.odm
					currentUsVersion = row.usVersion

					var plantId = -1;
					if ($('#selectPlantFirstCheckbox').is(':checked')) {
						var plantSearchGridSelected = $('#plantSearchInput').combogrid('grid').datagrid('getSelected');
						if (plantSearchGridSelected) {
							plantId = plantSearchGridSelected.id;
						}
					}
					refreshUserVisitRecordAndNotice(row.serialNum, plantId);
				},
				onLoadSuccess: function (data) {
					currentShowAutoParallelButton = data.showAutoParallelButton;
					if(currentShowAutoParallelButton && typeof hideAutoParallelButton == 'function') {
						$('#autoParallelButton').fadeIn();
						setTimeout(hideAutoParallelButton, 30000);
					}
					if (firstInverterSearchSuccess) {
						firstInverterSearchSuccess = false;
						$('#inverterSearchInput').combogrid('setValue', data.total > 0 ? data.rows[0].serialNum : '');

						var backupModuleInverterSn = currentModuleInverterSn;

						if (data.total > 0) {
							currentModuleInverterSn = data.rows[0].serialNum;
							currentDatalogSn = data.rows[0].datalogSn;
							currentProtocolVersion = data.rows[0].protocolVersion;
							currentPhase = data.rows[0].phase;
							currentDeviceType = data.rows[0].deviceType;
							currentSubDeviceType = data.rows[0].subDeviceType;
							currentPowerRating = data.rows[0].powerRating;
							currentDtc = data.rows[0].dtc;
							currentVoltClass = data.rows[0].voltClass;
							currentMachineType = data.rows[0].machineType;
							currentMeterType = data.rows[0].meterType;
							currentParallelEnabled = data.rows[0].parallelEnabled;
							currentMasterOrSlaveNotOffGrid = (currentParallelEnabled ? data.rows[0].masterOrSlave : -1);
							currentComposedPhaseNotOffGrid = (currentParallelEnabled ? data.rows[0].composedPhase : -1);
							currentStandard = data.rows[0].standard;
							currentSlaveVersion = data.rows[0].slaveVersion;
							currentFwVersion = data.rows[0].fwVersion;
							currentHardwareVersion = data.rows[0].hardwareVersion;
							currentBatteryType = data.rows[0].batteryType;
							currentWithbatteryData = data.rows[0].withbatteryData;
							currentBindTigoInfo = data.rows[0].bindTigoInfo
							currentBindPhnixInfo = data.rows[0].bindPhnixInfo
							currentBindChargePoint = data.rows[0].bindChargePoint
							currentAllowExport2Grid = data.rows[0].allowExport2Grid
							currentSetG98Already = data.rows[0].setG98Already
							currentOdm = data.rows[0].odm
							currentUsVersion = data.rows[0].usVersion
						}

						//Update visit record
						var plantId = -1;
						if ($('#selectPlantFirstCheckbox').is(':checked')) {
							var plantSearchGridSelected = $('#plantSearchInput').combogrid('grid').datagrid('getSelected');
							if (plantSearchGridSelected) {
								plantId = plantSearchGridSelected.id;
							}
						}

						if (!firstLoad || (backupModuleInverterSn != data.rows[0].serialNum)) {
							isInit = false;
							refreshUserVisitRecordAndNotice(data.total > 0 ? data.rows[0].serialNum : '', plantId);
						} else {
							firstLoad = false
						}
					}
				},
				columns: [[
					{field: 'serialNum', title: 'Serial number', width: 80}
				]]
			});
		}
	});

	let isInit = true
	var inverterImgSrc = '';
	var hostDeviceImgSrc = '';
	var midboxImgSrc = '';
	var parallelInverterImgPath = '';
	var platform = '';

	async function refreshUserVisitRecordAndNotice(serialNum, plantId) {
		userVisitRecordPlantId = plantId;
		userVisitRecordSerialNum = serialNum;

		if(!isInit) {
			await new Promise((resolve) => {
				$.post(baseUrl + '/api/userVisit/update', {plantId: plantId, serialNum: serialNum}, function (response) {
					if (response.success) {
						userVisitRecordPlantId = response.plantId;
						resolve(true)
						if (typeof (onDeviceChangedDone) == "function") {
							onDeviceChangedDone(serialNum);
						}
					}
				}, 'json');
			})
		} else {
			isInit = false
		}

		var needInverterImg = $('.hasInvertImg .inverterImg').length
		if (needInverterImg) {
			inverterImgSrc = false;
			// let defaultValue = false
			var hasCustomPlatformImg = false;
			let customPlatform = '';
			if (hasCustomPlatformImg) {
				var monitorDirPath = '/web/platform/' + customPlatform + '/img/monitor/plant/deviceIcon/';
				var midboxDirPath = '/web/platform/' + customPlatform + '/img/monitor/midbox/';
			} else {
				var monitorDirPath = '/web/img/monitor/plant/deviceIcon/';
				var midboxDirPath = '/web/img/monitor/midbox/';
			}
			var isPowerZone = false
			
			platform = 'LUX_POWER'
			var getaImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_geta.png' : 'icon_geta_mid.png');
			var minusFlexBossImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_minus_flex_boss.png' : 'icon_minus_flex_boss_mid.png');
			var acChargerImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_ac_charger.png' : 'icon_ac_charger_mid.png');
			var offGridInverterImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'inverter_off_grid_20231003.png' : 'inverter_off_grid_mid.png');
			var offGridUsInverterImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_sna_us_6k_20231004.png' : 'icon_sna_us_6k_mid.png');
			var sna6000UsAioImagePath = monitorDirPath + 'icon_sna_6000_us_aio.png';
			var ecoBeast6000ImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_eco_beast_6000.jpg' : 'icon_eco_beast_6000_mid.jpg');
			var ecoBeast6000_6_5ImagePath = monitorDirPath + 'icon_eco_beast_6000_6.5_5.png';
			var allInOneInverterImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'inverter_all_in_one_20240614.png' : 'inverter_all_in_one_20240614_mid.png');
			var trip12KInverterImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_inverter_trip.png' : 'icon_inverter_trip_mid.png');
			var trip12KLBInverterImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_inverter_trip_LB.png' : 'icon_inverter_trip_LB_mid.png');
			var trip12KHBInverterImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_inverter_trip_HB.png' : 'icon_inverter_trip_HB_mid.png');
			var midboxDirPathImagePath = midboxDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg) ? 'deviceIcon_20240729.png' : 'deviceIcon_20240729_mid.png');

			var gen3_6k_6ImagePath = monitorDirPath + 'icon_gen_3_6k_6.png';
			var gen3_6k_6_5ImagePath = monitorDirPath + 'icon_gen_3_6k_6.5.png';
			var gen3_6kImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_gen_3_6k.png' : 'icon_gen_3_6k_mid.png');
			var gen7_10kImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_gen_7_10k.png' : 'icon_gen_7_10k_mid.png');
			var gen7_10k_10ImagePath = monitorDirPath + 'icon_gen_7_10k_10.png';
			var sna12kEuImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_sna_12k_eu.png' : 'icon_sna_12k_eu_mid.png');
			var sna12kUsImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_sna_12k_us.png' : 'icon_sna_12k_us_mid.png');
			var sna2_12kImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'sna2_12k.png' : 'sna2_12k_mid.png');
			var sna6k_6_6_5ImagePath = monitorDirPath + 'icon_sna_6_6k_6.5.png';
			var sna12k_12_6ImagePath = monitorDirPath + 'icon_sna_12k_12.6.png';

			var type6_8_10k_euImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_type6_8_10k_eu.png' : 'icon_type6_8_10k_eu_mid.png');
			var type6_12k_euImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_type6_12k_eu.png' : 'icon_type6_12k_eu_mid.png');
			var type6_8_10k_usImagePath = monitorDirPath + ((platform == 'LUX_POWnER' || hasCustomPlatformImg || isPowerZone) ? 'icon_type6_8_10k_us.png' : 'icon_type6_8_10k_us_mid.png');
			var type6_12k_usImagePath = monitorDirPath + ((platform == 'LUX_POWER' || hasCustomPlatformImg || isPowerZone) ? 'icon_type6_12k_us.png' : 'icon_type6_12k_us_mid.png');

			parallelInverterImgPath = monitorDirPath + 'icon_inverter_parallel.jpg';
			if(platform == 'CROWN') {
				if(currentDeviceType == '10' && currentPowerRating == 6) {
					parallelInverterImgPath = monitorDirPath + 'icon_gen_3_6k_6_parallel.png';
				}
				if(currentDeviceType == '3' && currentPowerRating == 7) {
					parallelInverterImgPath = monitorDirPath + 'icon_sna_6_6k_6.5_parallel.png';
				}
				if(currentDeviceType == '10' && currentPowerRating == 7) {
					parallelInverterImgPath = monitorDirPath + 'icon_gen_3_6k_6.5_parallel.png';
				}
				if(currentDeviceType == '8' && currentPowerRating == 5) {
					parallelInverterImgPath = monitorDirPath + 'icon_gen_7_10k_10_parallel.png';
				}
				if(currentDeviceType == '11' && currentPhase == 1 && currentDtc == 0) {
					parallelInverterImgPath = monitorDirPath + 'icon_sna_12k_12.6_parallel.png';
				}
				if(currentDeviceType == 3 && isEcoBeast6k()) {
					if((currentPowerRating == 7 || currentPowerRating == 0) && platform == 'CROWN') {
						parallelInverterImgPath = monitorDirPath + 'icon_eco_beast_6000_6.5_5_parallel.png';
					}
				}
			}
			let notAcEssSrc = ''
			if (true) {
				if (true) {
					notAcEssSrc = "icon_inverter.png"
				}
				if (false) {
					notAcEssSrc = "icon_inverter_mid.png"
				}
			} else {
				notAcEssSrc = "icon_inverter.png"
			}

			if (currentDeviceType == 2) {
				inverterImgSrc = resourceBaseUrl + acChargerImagePath;
			} else {
				if (currentDeviceType == 3) {
					if(isSna6kUsAio()) {
						inverterImgSrc = resourceBaseUrl + sna6000UsAioImagePath;
					} else if(isEcoBeast6k()) {
						inverterImgSrc = resourceBaseUrl + ecoBeast6000ImagePath;
						if((currentPowerRating == 7 || currentPowerRating == 0) && platform == 'CROWN') {
							inverterImgSrc = resourceBaseUrl + ecoBeast6000_6_5ImagePath;
						}
					} else if (currentSubDeviceType == 131) {
						inverterImgSrc = resourceBaseUrl + offGridUsInverterImagePath;
					} else {
						inverterImgSrc = resourceBaseUrl + offGridInverterImagePath;
					}
					if(currentPowerRating == 7 && platform == 'CROWN') {
						inverterImgSrc = resourceBaseUrl + sna6k_6_6_5ImagePath;
					}
				} else if (currentDeviceType == 11) {
					if (currentSubDeviceType == 1111) {
						inverterImgSrc = resourceBaseUrl + sna12kUsImagePath;
					} else if(currentMachineType == 1) {
						inverterImgSrc = resourceBaseUrl + sna2_12kImagePath;
					} else {
						inverterImgSrc = resourceBaseUrl + sna12kEuImagePath;
						if(platform == 'CROWN') {
						    inverterImgSrc = resourceBaseUrl + sna12k_12_6ImagePath;
						}
					}
					// if(platform == 'CROWN' && currentPowerRating == '') {
					// 	inverterImgSrc = resourceBaseUrl + sna12k_12_6ImagePath;
					// }
				} else if (currentDeviceType == 6) {
					if(currentMachineType == 1) {
						inverterImgSrc = resourceBaseUrl + minusFlexBossImagePath;
					} else {
						if (currentSubDeviceType == 161) {
							inverterImgSrc = resourceBaseUrl + type6_8_10k_euImagePath;
						} else if (currentSubDeviceType == 162) {
							inverterImgSrc = resourceBaseUrl + type6_12k_euImagePath;
						} else if (currentSubDeviceType == 163) {
							inverterImgSrc = resourceBaseUrl + type6_8_10k_usImagePath;
						} else if (currentSubDeviceType == 164) {
							inverterImgSrc = resourceBaseUrl + type6_12k_usImagePath;
						} else {
							inverterImgSrc = resourceBaseUrl + monitorDirPath + notAcEssSrc;
						}
					}
				} else if (currentDeviceType == 7) {
					inverterImgSrc = resourceBaseUrl + allInOneInverterImagePath;
				} else if (currentDeviceType == 8) {
					inverterImgSrc = resourceBaseUrl + gen7_10kImagePath;
					if(platform == 'CROWN' && currentPowerRating == '5') {
						inverterImgSrc = resourceBaseUrl + gen7_10k_10ImagePath;
					}
				} else if (currentDeviceType == 10) {
					inverterImgSrc = resourceBaseUrl + gen3_6kImagePath;
					if(platform == 'CROWN' && currentPowerRating == 6) {
						inverterImgSrc = resourceBaseUrl + gen3_6k_6ImagePath;
					}
					if(platform == 'CROWN' && currentPowerRating == 7) {
						inverterImgSrc = resourceBaseUrl + gen3_6k_6_5ImagePath;
					}
				} else if (currentPhase == 3 && currentDeviceType == 0) {
					inverterImgSrc = resourceBaseUrl + trip12KInverterImagePath;
					if((platform == 'LUX_POWER' || !hasCustomPlatformImg) && isTripLv()) {
						inverterImgSrc = resourceBaseUrl + trip12KLBInverterImagePath;
					}
					if((platform == 'LUX_POWER' || !hasCustomPlatformImg) && isTripHv()) {
						inverterImgSrc = resourceBaseUrl + trip12KHBInverterImagePath;
					}
				} else if (currentDeviceType == 13) {
					inverterImgSrc = resourceBaseUrl + getaImagePath;
				} else if (currentDeviceType == 9) {
					midboxImgSrc = resourceBaseUrl + midboxDirPathImagePath;
					hostDeviceImgSrc = resourceBaseUrl + type6_12k_usImagePath
				} else {
					inverterImgSrc = resourceBaseUrl + monitorDirPath + notAcEssSrc;
				}
			}
		}
		if (!selectPlantId) {
			$('#favoriteBtn').hide()
		} else {
			$('#favoriteBtn').show()
		}
		onDeviceChanged(serialNum);					//Custom method implements by page
	}

	function isLowVoltBattery() {
		return currentVoltClass == 0;
	}

	function isHighVoltBattery() {
		return !isLowVoltBattery();
	}

	function isHybird() {
		return currentDeviceType == 0 && currentPhase != 3;
	}

	function isSna2_12k() {
		return currentDeviceType == 3 && currentMachineType == 1
	}
	
	function isTrip12K() {
		return currentDeviceType == 0 && currentPhase == 3;
	}

	function isMega24K() {
		return (currentDeviceType == 4 && currentDtc == 1);
	}

	function isTripHv() {
		return isTrip12K() && isHighVoltBattery();
	}

	function isTripLv() {
		return isTrip12K() && isLowVoltBattery();
	}

	function isSnaSeries() {
		return (currentDeviceType == 3 || currentDeviceType == 11);
	}

	function isSplitPhaseOffGrid() {
		return isSnaSeries() && currentDtc == 1;
	}

	function isEcoBeast6k() {
		return currentDeviceType == 3 && !(currentDtc == 1) && currentMachineType == 4;
	}

	function isSna6kUsAio() {
		return currentDeviceType == 3 && currentDtc == 1 && currentMachineType == 4;
	}

	function isFlexBoss18() {
		return currentDeviceType == 6 && currentSubDeviceType == 163 && currentPowerRating == 9
	}

	function isFlexBoss21() {
		return currentDeviceType == 6 && currentSubDeviceType == 164 && currentPowerRating == 8
	}

	function isGen2_7_14k() {
		return currentDeviceType == 8 && currentPhase == 1 && currentStandard == 'FAAC'
	}

	function checkType6Series() {
		return (currentDeviceType == 6 || currentDeviceType == 7 || currentDeviceType == 8 || currentDeviceType == 10 || isTrip12K());
	}

	function isLxpUS12k() {
		return currentDeviceType == 6 && currentPhase == 1 && currentSubDeviceType == 164;
	}
	
	function is12kHybridMinus() {
		return currentDeviceType == 6 && currentPhase == 1 && currentMachineType == 1;
	}
	
	function is6000XP() {
		return currentDeviceType == 3 && currentDtc == 1 && currentPhase == 1 && currentMachineType == 0
	}
	
	function is12000XP(){
		return currentDeviceType == 11 && currentPhase == 1 && currentDtc == 1
	}
	function is12KPV() {
		return currentDeviceType == 6 && currentPhase == 1 && currentPowerRating == 2 && currentSubDeviceType == 163
	}
	function is18KPV() {
		return currentDeviceType == 6 && currentPhase == 1 && currentSubDeviceType == 164
	}

	function checkHybridFirmware20240811() {
		if (currentDeviceType == 0 && currentPhase != 3) {		//Hybrid
			if(((currentStandard == 'AAAA' || currentStandard == 'aAAA') && currentSlaveVersion >= 0x24 && currentFwVersion >= 0x20)
					|| ((currentStandard == 'AAAB' || currentStandard == 'aAAB') && currentSlaveVersion >= 0x1D && currentFwVersion >= 0x1A)) {
				return true;
			}
		}
		return false;
	}

	function checkShowPv3ViewByDevice() {
		if (currentDeviceType == 7) {			//All-In-One...
			return true;
		}
		if (currentPhase == 3 && currentDeviceType == 0 && currentPowerRating >= 3) {		//12K Trip...
			return true;
		}
		if (currentDeviceType == 6) {			//6-12K
			if (currentPowerRating == 3 || currentPowerRating == 6 || currentPowerRating == 8 || currentPowerRating >= 10) {
				return true;
			}
			if (currentModuleInverterSn && currentModuleInverterSn.substring(4, 6) == '54') {
				return true;
			}
			if (currentModuleInverterSn && (currentModuleInverterSn.startsWith('2134') || currentModuleInverterSn.startsWith('2184') || currentModuleInverterSn.startsWith('2044'))) {
				return true;
			}
		}
		return false;
	}
</script>

				
