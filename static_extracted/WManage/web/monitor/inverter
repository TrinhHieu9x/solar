<script language="JavaScript">
    $(document).ready(function() {
        getNoticeList()
    });

    function getNoticeList() {
        $.post(baseUrl + '/web/maintain/notification/showList', {deviceType: currentDeviceType,page: 1,rows: 999 }, function(response) {
            let content = ''
            if(response.success) {
                response.rows.forEach((item, index) => {
                    if(index == 0) {
                        content += '<div class="slide" onclick="openDialog(' + JSON.stringify(item).replace(/\"/g,"'") + ')">'+(item.versionCode?item.versionCode+'_':'')+moment(item.time).format('YYYY-MM-DD')+': '+item.notification+'</div>'
                    } else {
                        content += '<div class="slide" onclick="openDialog(' + JSON.stringify(item).replace(/\"/g,"'") + ')" style="transform: translateY(100%);">'+(item.versionCode?item.versionCode+'_':'')+moment(item.time).format('YYYY-MM-DD')+': '+item.notification+'</div>'
                    }
                })
                if(!content) {
                    $('.noticeBox'). hide()
                } else {
                    $('#carousel').append(content)
                }
                initSlide()
            } else {
                $('.noticeBox'). hide()
            }
        }, 'json');
    }

    function initSlide() {
        var $carousel = $('#carousel');
        var $slides = $carousel.find('.slide');
        var currentSlide = 0;
        var slideInterval = setInterval(nextSlide, 8000); // 每3秒滚动到下一张图片

        function nextSlide() {
            var nextSlide = (currentSlide + 1) % $slides.length;
            $slides.eq(currentSlide).css('transform', 'translateY(100%)');
            $slides.eq(nextSlide).css('transform', 'translateY(0)');
            currentSlide = nextSlide;
        }

        $carousel.hover(function() {
            clearInterval(slideInterval);
        }, function() {
            slideInterval = setInterval(nextSlide, 8000);
        });
    }

    function openDialog(item) {
        if(item.versionCode) {
            $('#versionCodeHolder').show()
            $('#versionCode').text(item.versionCode)
        } else {
            $('#versionCodeHolder').hide()
        }
        $('#publishTme').text(moment(item.time).format('YYYY-MM-DD'))
        $('#noticeContent').text(item.notification)
        $('#noticeModal').modal();
    }
</script>

