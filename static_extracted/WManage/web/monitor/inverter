	<script type="text/javascript">
		var sessionChartValue = JSON.parse(`{"records":{"3_-1_1_0":{"barAcCouple":false,"barBattCharge":true,"barBattChargeValue":true,"barBattery":true,"barConsumption":true,"barExportToGrid":false,"barImportToUser":true,"barNetEnergyValue":false,"barSolarProduction":true,"deviceType":3,"deviceTypeValue":3,"dtc":0,"dtcValue":0,"lineAcCouple":true,"lineBattery":true,"lineConsumption":true,"lineGrid":true,"lineSOC":true,"lineSolarPV":true,"phase":1,"phaseValue":1,"recordId":493,"subDeviceTypeValue":-1,"userId":98345}}}`).records
		let isToLucChat = true
		function toLuxChat(){
			isToLucChat = !isToLucChat
			if (!isToLucChat) {
				setTimeout(function () {
					$('[data-toggle="popover"]').popover('hide');
				},4000)
			}
			return true
		}

		$(function(){
			//进入首页默认显示Q&A提示4S
			$('[data-toggle="popover"]').popover();
			$('[data-toggle="popover"]').popover('show');
			setTimeout(function () {
				$('[data-toggle="popover"]').popover('hide');
			},4000)

			//滚动的时候不显示Q&A提示
			$(window).scroll(function (e) {
				let scrollTop = $(document).scrollTop()
				$('[data-toggle="popover"]').popover('hide');
			})
		});
	</script>
