<script language="JavaScript">
	var userVisitRecordPlantId = '122555';
	var userVisitRecordSerialNum = '3233630034';

	var selectPlantId = -1;
	var selectPlantName = '';

	var currentModuleInverterSn = '';
	var currentDatalogSn = '';
	var currentProtocolVersion = -1;
	var currentPhase = 1;
	var currentDeviceType = -1;
	var currentSubDeviceType = -1;
	var currentPowerRating = -1;
	var currentDtc = -1;
	var currentVoltClass = -1;
	var currentMachineType = -1;
	var currentMeterType = -1;
	var currentParallelEnabled = false;
	var currentMasterOrSlaveNotOffGrid = -1;
	var currentComposedPhaseNotOffGrid = -1;

	var currentStandard = '';
	var currentSlaveVersion = -1;
	var currentFwVersion = -1;
	var currentHardwareVersion = -1;

	var currentBatteryType = '';
	var currentWithbatteryData = false;

	var currentBindTigoInfo = false;
