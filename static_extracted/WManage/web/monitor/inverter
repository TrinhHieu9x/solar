<script language="JavaScript">
	var userId = '98345'
	var headerPlatform = 'LUX_POWER';

	function formatDate(date) {
		return moment(date).format("YYYY-MM-DD");
	}

	$.fn.datebox.defaults.formatter = formatDate;

	var language = 'en';
	var language4Datetimepicker = language.replace('_', '-');

	var baseUrl = '/WManage';
	var resourceBaseUrl = 'https://resource.solarcloudsystem.com/WManage';

	
		var isAdmin = false;
	
	
	
		var isMaintain = false;
	
	
	
		var isDevAccount = false;
	
	

	
		var isEG4Admin = false;
	
	

	
		var isOwnerLevel = false;
	
	

	
	
		var isInstallerLevel = false;
	

	var gigabiz1User = false;
	

	
	
		var isSecondCluster = false;
	

	
	
		var isIndiaCluster = false;
	

	var redisRunning = false;

	
	
		var isViewer = true;
		var plantListUrl = baseUrl + '/web/config/plant/list/viewer';
	
	
	var isPowerZone = false
	

	var currentClusterId = 1;

	var serverTimezone = 8;

	var defaultLng = 114.062944;
	var defaultLat = 22.550805;

	var noPermissionText = 'No permission';

	var successText = 'Success';
	var failedText = 'Failed';

	var setSuccessText = 'Set successfully';

	var beforePageText = 'Page';
	var afterPageText = 'of {pages}';
	var displayMsg = 'Displaying {from} to {to} of {total} items';

	
	
	var isEG4Platform = false;
	

	
	
	var isFortressPlatform = false;
	

	
	
	var isGTECPlatform = false;
	

	function notAllowSelect(index, row) {
		return false;
	}

	function defaultRowStyler(index, row) {
		if (index % 2 == 1) {
			return 'background: rgb(245, 252, 222);';
		}
	}

	function tableLostFormatter(value, row) {
		if (value) {
			return '<span style="color: gray">Lost</span>';
		} else {
			return '<span style="color: green">Connected</span>';
		}
	}

	function plantSearchNameFormatter(value, row) {
		return value + (row.endUserAccount ? (' - ' + row.endUserAccount) : '') + (row.createDate ? (' - ' + row.createDate) : '');
	}

	function deviceOverviewJumpFormatter(value, row) {
		if (row && value) {
			return '<a href="javascript:jumpDeviceOverview(' + row.plantId + ')">' + value + '</a>';
		}
		return '';
	}

	function jumpDeviceOverview(plantId) {
		$.post(baseUrl + '/api/userVisit/update', {plantId: (plantId ? plantId : -1)}, function (response) {
			window.location.href = baseUrl + '/web/overview/plant?autoSelectPlant=true';
		}, 'json');
	}

	function inverterJumpSerialNumFormatter(value, row) {
		return '<a href="javascript:jumpSerialNum(' + row.plantId + ', \'' + value + '\')">' + value + '</a>';
	}

	function tableStatusFormatter(value, row) {
		if (value) {
			var imgElementHtml = '<img style="margin-right: 5px;" width="16" height="16" src="' + resourceBaseUrl + '/web/img/format/status/status_' + value + '.png" />';
			switch (value) {
				case 'offline':
					return '<div>' + imgElementHtml + '<span style="position: relative; top: 2px;">Offline</span></div>';
				case 'warning':
					return '<div>' + imgElementHtml + '<span style="position: relative; top: 2px;">Notice</span></div>';
				case 'error':
					return '<div>' + imgElementHtml + '<span style="position: relative; top: 2px;">Error</span></div>';
				case 'normal':
					return '<div>' + imgElementHtml + '<span style="position: relative; top: 2px;">Normal</span></div>';
			}
		}
		return '';
	}

	function jumpSerialNum(plantId, serialNum) {
		$.post(baseUrl + '/api/userVisit/update', {
			plantId: (plantId ? plantId : -1),
			serialNum: serialNum
		}, function (response) {
			window.open(baseUrl + '/web/monitor/inverter', '_blank');
		}, 'json');
	}

	function booleanFormatter(value, row) {
		if (typeof (value) == 'undefined') {
			return '';
		}
		return value ? 'Yes' : 'No';
	}

	function successFormatter(value, row) {
		if (typeof (value) == 'undefined') {
			return '';
		}
		if (value) {
			return '<span style="color: green;">Success</span>';
		} else {
			return '<span style="color: red;">Failed</span>';
		}
	}

	function doneFormatter(value, row) {
		if (typeof (value) == 'undefined') {
			return '';
		}
		if (value) {
			return '<span style="color: green;">Done</span>';
		} else {
			return 'Undone';
		}
	}

	function checkboxFormatter(value, row) {
		return value ? '<input type="checkbox" disabled="disabeld" checked="checked"/>' : '<input type="checkbox" disabled="disabeld"/>';
	}

	function tableMoneyFormatter(value, row) {
		return toDecimal2(value / 100);
	}

	function initTablePaginationI18n(tableId) {
		var pagination = $(tableId).datagrid('getPager');
		$(pagination).pagination({
			beforePageText: beforePageText,
			afterPageText: afterPageText,
			displayMsg: displayMsg
		});
	}
</script>

